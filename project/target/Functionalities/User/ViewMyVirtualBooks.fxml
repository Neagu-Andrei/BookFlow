                              <TableColumn fx:id="col_accessKey" prefWidth="110.0" text="Access key" />
                          </columns>
                      </TableView>
                  <Button onMouseClicked="#pressedViewBtn" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" text="View" />
