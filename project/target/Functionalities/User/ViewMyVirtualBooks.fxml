<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="492.0" prefWidth="1052.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ro.mta.libraryproject.Controllers.ViewMyVirtualBooksController">
    <children>
      <HBox layoutX="47.0" layoutY="94.0" spacing="20.0">
         <children>
            <VBox alignment="CENTER" prefHeight="305.0" prefWidth="491.0" spacing="10.0">
               <children>
                      <TableView prefHeight="261.0" prefWidth="491.0" fx:id="table">
                          <columns>
                              <TableColumn fx:id="col_ID" prefWidth="25.0" text="ID" />
                              <TableColumn fx:id="col_ISBN" prefWidth="52.0" text="ISBN" />
                              <TableColumn fx:id="col_title" prefWidth="90.0" text="Title" />
                              <TableColumn fx:id="col_authors" prefWidth="120.0" text="Authors" />
                              <TableColumn fx:id="col_genre" prefWidth="47.0" text="Genre" />
